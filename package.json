{
  "name": "employee-explorer",
  "version": "1.0.0",
  "description": "Recursive employee explorer (multiway tree) using React along with duplicates removal",
  "scripts": {
    "start:server": "nodemon server",
    "start:client": "react-scripts start",
    "start": "concurrently -c \"bgBlue.bold,bgMagenta.bold\" \"npm:start:server\" \"npm:start:client\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --cache --cache-location \".cache/.eslintcache/\"",
    "format": "npm run lint -- --fix",
    "test": "react-scripts test",
    "check": "npm run lint && cross-env CI=true npm run test",
    "build": "npm run check && react-scripts build",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": "10.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pgmanutd/employee-explorer.git"
  },
  "keywords": [
    "react",
    "hooks",
    "custom-hooks",
    "recursive",
    "recursion",
    "react-recursion",
    "employee",
    "explorer",
    "employee-explorer",
    "tree",
    "multiway",
    "multiway-tree",
    "multi-way",
    "multi-way-tree",
    "duplicates",
    "duplicates-removal",
    "remove-duplicates",
    "traversal",
    "tree-traversal"
  ],
  "author": "pgmanutd <prashantmanunited@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pgmanutd/employee-explorer/issues"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-plugin-prettier": "^3.1.0",
    "history": "^4.9.0",
    "http-proxy-middleware": "^0.19.1",
    "jest-localstorage-mock": "^2.4.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": 2
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
